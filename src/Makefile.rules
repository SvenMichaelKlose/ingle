CC65_HOME ?= /usr/local
CC65_INCLUDE = $(CC65_HOME)/share/cc65/include/
AS      = $(CC65_HOME)/bin/ca65
CC      = $(CC65_HOME)/bin/cl65
AR      = $(CC65_HOME)/bin/ar65
LD      = $(CC65_HOME)/bin/ld65
ASFLAGS = -I/usr/local/share/cc65/asminc/
CFLAGS  = -O -r -Or -Os -Oi -I/usr/local/share/cc65/include/ -Wa -I/usr/local/share/cc65/asminc/
LDFLAGS = -m $(PROGRAM).map

EXOMIZER ?= exomizer-2.0.10
